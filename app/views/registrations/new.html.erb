<h1>Registrations#new</h1>
<%= form_with model: @user, url: sign_up_path do |f|  %>
    <fieldset>
    <%= f.label :email %>
    <%= f.text_field :email %>
	</fieldset>
	<fieldset>
    <%= f.label :password %>
    <%= f.password_field :password %>
    </fieldset>
    <fieldset>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
	</fieldset>
    <%= f.submit "Sign Up" %>
<% end %>